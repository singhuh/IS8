@model IEnumerable<IS8.Models.Title>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Job Titles</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TitleName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
        {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TitleName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.TitleName }) |
            @Html.ActionLink("Details", "Details", new { id = item.TitleName }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.TitleName })
        </td>
    </tr>
        }

</table>
